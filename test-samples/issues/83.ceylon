void issue83() {
    variable Integer i = 0;
    print(- --i);
    print(+ ++i);
    print(- ++i);
    print(+ --i);
    print(i-- - i);
    print(i - --i);
    print(i - -i);
    print(-i--);
    print(i-- - --i);
    print(i++ + i);
    print(i + ++i);
    print(i + +i);
    print(+i++);
    print(i++ + ++i);
}
